<Window x:Class="CirclesApp.TeacherWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CirclesApp"
        mc:Ignorable="d"
        Title="TeacherWindow" Height="450" Width="800" ResizeMode="NoResize">
    <Grid Margin="0,0,0,0">
        <Frame x:Name="TeacherWindowFrame" Content="Frame"  Margin="149,0,0,0" Navigated="TeacherWindowFrame_Navigated"  NavigationUIVisibility="Hidden">

        </Frame>
        <Image Margin="0,0,600,0"  Source="/Rectangle 2 (1).png" Stretch="Fill"/>
        <Label x:Name="TicherName" Width="149" Margin="5,65,0,0" HorizontalAlignment="Left" FontSize="15" Foreground="#4B4C4E" FontWeight="DemiBold"  Content=""/>
        <Image Margin="-10,0,650,350" Width="110" Height="35" Source="/Group 1.png" Stretch="Fill"/>
        <Image Margin="0,0,640,85" Source="/Кнопка входа (1).png" Stretch="Fill" Width="120" Height="29" MouseDown="Image_MouseDown_1"/>
        <Image Margin="0,0,640,155" Source="/Кнопка входа.png" Width="120" Height="29" Stretch="Fill" MouseDown="Image_MouseDown"/>
    </Grid>
</Window>
